<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from LegalServiceService WSDL service -->

  <service name="LegalService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://legal"/>
      <parameter name="wsdlServiceElement" value="LegalServiceService"/>
      <parameter name="schemaQualified" value="http://legal"/>
      <parameter name="wsdlServicePort" value="LegalService"/>
      <parameter name="className" value="legal.LegalServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="LegalService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="isLegal" qname="operNS:isLegal" xmlns:operNS="http://legal" returnQName="retNS:isLegalReturn" xmlns:retNS="http://legal" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://legal" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="isLegal"/>

  </service>
</deployment>
